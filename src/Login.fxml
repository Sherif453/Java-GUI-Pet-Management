<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <Accordion layoutX="191.0" layoutY="72.0" />
      <Label layoutX="237.0" layoutY="58.0" prefHeight="54.0" prefWidth="127.0" text="LOGIN">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="130.0" layoutY="138.0" prefHeight="25.0" prefWidth="340.0" />
      <PasswordField fx:id="passwordField" layoutX="130.0" layoutY="200.0" prefHeight="25.0" prefWidth="340.0" />
      <Label fx:id="statusLabel" layoutX="340.0" layoutY="255.0" text="Status" />
      <Button fx:id="loginButton" layoutX="265.0" layoutY="251.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
      <Button fx:id="registerLink" layoutX="258.0" layoutY="294.0" mnemonicParsing="false" onAction="#handleRegisterLink" text="Register" />
   </children>
</AnchorPane>
